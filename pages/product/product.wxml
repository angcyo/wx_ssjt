<!--pages/product/product.wxml-->
<import src="../../wxParse/wxParse.wxml" />
<view>
  <view class='detail'>
    <image wx:for="{{imgs}}" src="{{item}}" class='image' mode='widthFix' bindtap='onImageClick' data-src="{{item}}" data-srcs="{{imgs}}"></image>
  </view>
  <progress percent="{{progress}}" color="#FE7272" activeColor='#FE7272' backgroundColor='#fff' active='true' class='progress' stroke-width='5' />
</view>